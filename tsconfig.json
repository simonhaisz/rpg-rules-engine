{
    "compilerOptions": {
        "alwaysStrict": true,
        "lib": [
            "esnext",
        ],
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "outDir": "./out",
        "skipLibCheck": false
    },
    "include": [
        "src/**/*.ts"
    ]
}